{
  "rabbitmq":{
    "host":"127.0.0.1",
    "user":"example",
    "password":"Example",
    "queues":
    [
      {
        "name":"test_queue",
        "declare":true,
        "durable":true,
        "exclusive":false,
        "internal":false,
        "auto_delete":false,
        "nowait":true
      }
    ],
    "exchanges":
    [
      {
        "declare":true,
        "name":"test_queue_fanout",
        "type":"fanout",
        "bind":[
          {
            "name":"test_queue",
            "arguments":{
              "x-cancel-on-ha-failover":
              [
                "t",
                true
              ]
            }
          }
        ]
      }
    ],
    "qos":{
      "size":null,
      "count":1,
      "global":false
    },
    "max_workers":15,
    "min_workers":10,
    "worker_autoadjust":true,
    "autoadjust":{
      "destroy":10,
      "create":90
    }
  },
  "daemon":false,
  "user":"root",
  "group":"root",
  "shared_memory":{
    "tmp_file":"/tmp/RMQ_WORKER.{n}.tmp"
  },
  "log_base":"RMQ",
  "log":[
    {
      "type":"file",
      "level":"notice",
      "file":"./logs/RMQ.log",
      "rotate":true,
      "max_logs":5
    },
    {
      "type":"stdout",
      "level":"warning"
    }
  ]
}
